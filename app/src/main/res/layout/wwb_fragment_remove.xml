<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:paddingTop="20dp"
             android:background="#f8f8f8"
             tools:context=".fragment.RemoveFragment">

     <RelativeLayout

         android:layout_width="match_parent"
         android:layout_height="match_parent">

         <LinearLayout
             android:orientation="vertical"
             android:layout_width="match_parent"
             android:layout_height="wrap_content">
             <RelativeLayout
                 android:layout_marginLeft="15dp"
                 android:layout_marginRight="15dp"
                 android:background="@drawable/wwb_e2_bg"
                 android:layout_width="match_parent"
                 android:layout_height="140dp">
                 <RelativeLayout
                     android:elevation="5dp"

                     android:layout_width="match_parent"
                     android:layout_height="70dp">
                     <RelativeLayout
                         android:id="@+id/remove_addr1"
                         android:layout_toLeftOf="@+id/line1"
                         android:layout_width="match_parent"
                         android:layout_height="match_parent">
                         <ImageView
                             android:layout_centerVertical="true"
                             android:id="@+id/ic_locate"
                             android:layout_marginRight="10dp"
                             android:layout_marginLeft="10dp"
                             android:src="@mipmap/wwb_location_choose"
                             android:layout_width="@dimen/margin_15dp"
                             android:layout_height="@dimen/length_view_30dp"/>
                         <TextView

                             android:gravity="center_vertical"
                             android:id="@+id/now"
                             android:maxLines="2"
                             android:ellipsize="end"
                             android:layout_marginRight="@dimen/margin_10dp"
                             android:layout_centerVertical="true"
                             android:textSize="@dimen/text_size_14sp"
                             android:layout_toRightOf="@id/ic_locate"
                             android:text="请输入当前地址"
                             android:textColor="@color/color_999"
                             android:layout_width="match_parent"
                             android:layout_height="match_parent"/>
                     </RelativeLayout>

                     <View
                         android:id="@+id/line1"
                         android:layout_marginBottom="15dp"
                         android:layout_marginTop="15dp"
                         android:layout_marginRight="10dp"
                         android:layout_toLeftOf="@id/ceng"
                         android:background="#e2e2e2"
                         android:layout_width="1dp"
                         android:layout_height="match_parent">

                     </View>
                     <TextView
                         android:gravity="center"
                         android:layout_centerVertical="true"
                         android:id="@+id/ceng"
                         android:textColor="@color/color_999"
                         android:maxWidth="@dimen/margin_60dp"
                         android:text="楼层"
                         android:layout_marginRight="10dp"
                         android:layout_alignParentRight="true"
                         android:textSize="@dimen/text_size_14sp"
                         android:layout_width="@dimen/length_view_70dp"
                         android:layout_height="match_parent"/>
                 </RelativeLayout>
                 <View
                     android:layout_alignTop="@id/des"
                     android:background="#e2e2e2"
                     android:layout_width="match_parent"
                     android:layout_height="1dp"></View>
                 <RelativeLayout
                     android:id="@+id/des"
                     android:layout_alignParentBottom="true"
                     android:layout_width="match_parent"
                     android:layout_height="70dp">
                     <RelativeLayout
                         android:id="@+id/remove_addr2"
                         android:layout_toLeftOf="@id/line2"
                         android:layout_width="match_parent"
                         android:layout_height="match_parent">
                         <ImageView
                             android:layout_centerVertical="true"
                             android:id="@+id/iclocate"
                             android:layout_marginRight="10dp"
                             android:layout_marginLeft="10dp"
                             android:src="@mipmap/wwb_location_choose"
                             android:layout_width="@dimen/margin_15dp"
                             android:layout_height="@dimen/length_view_30dp"/>
                         <TextView
                             android:id="@+id/where"
                             android:maxLines="2"
                             android:ellipsize="end"
                             android:layout_marginRight="@dimen/margin_10dp"
                             android:gravity="center_vertical"
                             android:layout_centerVertical="true"
                             android:textSize="@dimen/text_size_14sp"
                             android:layout_toRightOf="@id/iclocate"
                             android:text="请输入目的地地址"
                             android:textColor="@color/color_999"
                             android:layout_width="match_parent"
                             android:layout_height="match_parent"/>
                     </RelativeLayout>

                     <View
                         android:id="@+id/line2"
                         android:layout_marginBottom="15dp"
                         android:layout_marginTop="15dp"
                         android:layout_marginRight="10dp"
                         android:layout_toLeftOf="@id/ceng2"
                         android:background="#e2e2e2"
                         android:layout_width="1dp"
                         android:layout_height="match_parent">

                     </View>

                     <TextView
                         android:id="@+id/ceng2"
                         android:layout_width="@dimen/length_view_70dp"
                         android:layout_height="match_parent"
                         android:layout_alignParentRight="true"
                         android:layout_centerVertical="true"
                         android:layout_marginRight="10dp"
                         android:gravity="center"
                         android:maxWidth="@dimen/margin_60dp"
                         android:text="楼层"
                         android:textColor="@color/color_999"
                         android:textSize="@dimen/text_size_14sp"/>
                 </RelativeLayout>

             </RelativeLayout>
             <LinearLayout
                 android:orientation="vertical"
                 android:background="@drawable/wwb_e2_bg"
                 android:layout_marginRight="@dimen/margin_15dp"
                 android:layout_marginLeft="@dimen/margin_15dp"
                 android:layout_marginTop="@dimen/margin_20dp"
                 android:layout_width="match_parent"
                 android:layout_height="200dp">
                 <RelativeLayout

                     android:layout_width="match_parent"
                     android:layout_height="50dp">
                     <TextView
                         android:layout_marginLeft="@dimen/margin_15dp"
                         android:id="@+id/tv_contact"
                         android:textColor="#444444"
                         android:layout_centerVertical="true"
                         android:text="联系人"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"/>
                     <EditText
                         android:maxLength="10"
                         android:id="@+id/et_contact"
                         android:hint="请输入联系人"
                         android:textColor="@color/color_999"
                         android:textSize="@dimen/text_size_14sp"
                         android:layout_marginLeft="35dp"
                         android:layout_marginRight="@dimen/margin_20dp"
                         android:layout_centerVertical="true"
                         android:layout_toRightOf="@id/tv_contact"
                         android:background="@color/white"
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"/>
                     <View
                         android:layout_alignParentBottom="true"
                         android:background="#e2e2e2"
                         android:layout_width="match_parent"
                         android:layout_height="1dp"></View>
                 </RelativeLayout>
                 <RelativeLayout

                     android:layout_width="match_parent"
                     android:layout_height="50dp">
                     <TextView
                         android:layout_marginLeft="@dimen/margin_15dp"
                         android:id="@+id/phone"
                         android:textColor="#444444"
                         android:layout_centerVertical="true"
                         android:text="联系电话"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"/>
                     <EditText
                         android:inputType="phone"
                         android:maxLength="11"
                         android:textColor="@color/color_999"
                         android:id="@+id/et_phone"
                         android:textSize="@dimen/text_size_14sp"
                         android:hint="请输入联系电话"
                         android:layout_marginLeft="@dimen/margin_20dp"
                         android:layout_marginRight="@dimen/margin_20dp"
                         android:layout_centerVertical="true"
                         android:layout_toRightOf="@id/phone"
                         android:background="@color/white"
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"/>
                     <View
                         android:layout_alignParentBottom="true"
                         android:background="#e2e2e2"
                         android:layout_width="match_parent"
                         android:layout_height="1dp"></View>
                 </RelativeLayout>
                 <RelativeLayout
                     android:id="@+id/time_layout"
                     android:layout_width="match_parent"
                     android:layout_height="50dp">
                     <TextView
                         android:layout_marginLeft="@dimen/margin_15dp"
                         android:id="@+id/tv_time"
                         android:textColor="#444444"
                         android:layout_centerVertical="true"
                         android:text="搬家时间"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"/>
                     <TextView
                         android:id="@+id/selTime"
                         android:text="点击选择计划搬家时间"
                         android:textColor="@color/color_999"
                         android:layout_marginLeft="@dimen/margin_20dp"
                         android:layout_marginRight="@dimen/margin_20dp"
                         android:layout_centerVertical="true"
                         android:layout_toRightOf="@id/tv_time"
                         android:background="@color/white"
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"/>
                     <View
                         android:layout_alignParentBottom="true"
                         android:background="#e2e2e2"
                         android:layout_width="match_parent"
                         android:layout_height="1dp"></View>
                 </RelativeLayout>
                 <RelativeLayout

                     android:layout_width="match_parent"
                     android:layout_height="50dp">
                     <TextView
                         android:layout_marginLeft="@dimen/margin_15dp"
                         android:id="@+id/beizhu"
                         android:textColor="#444444"
                         android:layout_centerVertical="true"
                         android:text="备注"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"/>
                     <EditText
                         android:textColor="@color/color_999"
                         android:id="@+id/et_desc"
                         android:textSize="@dimen/text_size_14sp"
                         android:hint="补充描述（选填）"
                         android:layout_marginLeft="@dimen/margin_50dp"
                         android:layout_marginRight="@dimen/margin_20dp"
                         android:layout_centerVertical="true"
                         android:layout_toRightOf="@id/beizhu"
                         android:background="@color/white"
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"/>

                 </RelativeLayout>
             </LinearLayout>
         </LinearLayout>

         <include

             android:layout_alignParentBottom="true"
             layout="@layout/wwb_bottom_include"
             android:layout_height="wrap_content"
             android:layout_width="wrap_content">

         </include>
     </RelativeLayout>

</FrameLayout>