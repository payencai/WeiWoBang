                android:textColor="@color/black"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_7dp"
                android:layout_gravity="center_horizontal"
                android:textSize="@dimen/text_size_9sp"
                android:text="跑腿"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/CompChartCell_mCenterLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:layout_marginLeft="@dimen/margin_10dp"
                android:textColor="@color/black"
                android:id="@+id/CompChartCell_mTextName"
                style="@style/form_item_title_font"
                android:text="暂无" />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp" />

            <TextView
                android:id="@+id/CompChartCell_mTextDescription"
                style="@style/block_item_title_font"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
