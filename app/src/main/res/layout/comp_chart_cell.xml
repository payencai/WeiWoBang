<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/CompChartCell_mLeftAndCenterLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/size_text_10dp"
            android:background="@color/white"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="@dimen/length_view_37dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="0dp"
                android:gravity="center">
                <ImageView
                    android:id="@+id/CompChartCell_mImgIcon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/location"/>
                <TextView
                    android:textColor="@color/black"
                    android:id="@+id/CompChartCell_mAddressType"
                    style="@style/form_item_text_font"
                    android:layout_marginLeft="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_7dp"
                    android:layout_gravity="center_horizontal"
                    android:textSize="@dimen/text_size_9sp"
                    android:text="送货地址"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/CompChartCell_mCenterLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:textColor="@color/black"
                    android:id="@+id/CompChartCell_mTextName"
                    style="@style/form_item_title_limit"
                    android:layout_width="wrap_content"
                    android:layout_marginRight="20dp"
                    android:text="待输入" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp" />

                <TextView
                    android:id="@+id/CompChartCell_mTextDescription"
                    style="@style/form_item_text_limit"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|left"
                    android:layout_marginRight="10dp"
                    android:text="" />

                <LinearLayout
                    android:id="@+id/CompChartCell_mLabelLy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:visibility="visible">
                    <TextView
                        android:textColor="@color/tv_666"
                        android:id="@+id/CompChartCell_mLabelPerson"
                        style="@style/form_item_text_font"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="" />
                    <TextView
                        android:id="@+id/CompChartCell_mLabelTel"
                        style="@style/block_item_title_font"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="" />

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

</merge>